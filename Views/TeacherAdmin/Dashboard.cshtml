@{
ViewData["Title"] = "Dashboard";
}
@model (int numGroups, int judgePerGroup, int SurveyResponses, int ItemsGraded401, int ItemsGraded413, int ItemsGraded414, int ItemsGraded455)

<br><br>
<div>
    <a href="/TeacherAdmin/JudgeSummary" class="plaintext-link">
        <div class="row">
            <div class="col-md-2">
                <h5 class="inter-heading"><b>Judges</b></h5>
            </div>
            <div class="col-md-10">
                <p class="text-nextex-paragraph inter-paragraph">
                    @Model.SurveyResponses/@(Model.numGroups * Model.judgePerGroup) Surveys Submitted
                </p>
            </div>
        </div>
        <div class="progress rounded-edges" style="height: 3rem;">
            <div class="progress-bar bg-nextex-button rounded-edges" role="progressbar" 
                 style="width: @(((double)Model.SurveyResponses / (Model.numGroups * Model.judgePerGroup)) * 100)%;" 
                 aria-valuenow="@(((double)Model.SurveyResponses / (Model.numGroups * Model.judgePerGroup)) * 100)" 
                 aria-valuemin="0" aria-valuemax="100"></div>
        </div>
    </a>
</div>
<br><br>
<div>
    <div class="row">
        <div class="col-md-2">
            <h5 class="inter-heading"><b>IS 401</b></h5>
        </div>
        <div class="col-md-10">
            <p class="text-nextex-paragraph inter-paragraph">
                @Model.ItemsGraded401/@Model.numGroups Rubrics Graded
            </p>
        </div>
    </div>
    <div class="progress rounded-edges" style="height: 3rem;">
        <div class="progress-bar bg-nextex-button rounded-edges" role="progressbar" 
             style="width: @(((double)Model.ItemsGraded401 / Model.numGroups) * 100)%;" 
             aria-valuenow="@(((double)Model.ItemsGraded401 / Model.numGroups) * 100)" 
             aria-valuemin="0" aria-valuemax="100"></div>
    </div>
</div>
<br><br>
