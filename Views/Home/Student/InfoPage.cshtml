@model Student

@{
    ViewData["Title"] = "Student_Info Page";
}

<div class="container">
    <div class="text-center mt-5">
        <h1 class="display-4">Your Group Information</h1>
    </div>
    
        <div class="row mt-4">
            <div class="col-md-3 font-weight-bold">First Name</div>
            <div class="col-md-3">@Model.first_name</div>
        </div>

        <div class="row">
            <div class="col-md-3 font-weight-bold">Last Name</div>
            <div class="col-md-3">@Model.last_name</div>
        </div>

        <div class="row">
            <div class="col-md-3 font-weight-bold">Student ID</div>
            <div class="col-md-3">@Model.student_netid</div>
        </div>

        <div class="row">
            <div class="col-md-3 font-weight-bold">Section Number</div>
            <div class="col-md-3">@Model.Student_Group.section_number</div>
            
        </div>

         <div class="row">
             <div class="col-md-3 font-weight-bold">Group Number</div>
             <div class="col-md-3">@Model.Student_Group.group_number</div>
        </div>



        <form id="form1" runat="server">
            <div class="row mt-4">
                <div class="col-md-12">
                    <div>
                    <iframe src="https://docs.google.com/spreadsheets/d/1MJ_KrKasUdK4L7vjhXGuWkzff0HA8kJhPIVnHLpxHcU/edit?usp=sharing;headers=false" width="100%" height="500"></iframe>    
                    </div>
                    
                </div>
            </div>
        </form>
         <footer class="row mt-4">
             <div class="col-md-12 text-center">
                 
                 <p class="text-muted">If any of this information is incorrect, please email blahblah@byu.edu</p>
             </div>
        </footer>
     </div>

</div>